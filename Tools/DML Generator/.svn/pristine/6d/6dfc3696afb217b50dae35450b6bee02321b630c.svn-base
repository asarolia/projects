<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Content/Scripts/jquery-1.6.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Scripts/jquery.json2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Scripts/SignalR.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/signalr/hubs")" type="text/javascript"></script>
</head>

<body>
    <div class ="header">
        <div class="title">
            DML Generation Tool
        </div>
    </div>
    <div style="width: 100%; height: 8px; float: left;">
            <div style="width: 39%; height: 8px; float: left; background-color: #FFCC66;">
            </div>
            <div style="width: 61%; height: 8px; float: left; background-color: #549635;">
            </div>
        </div>
    <div id="categories">
            <div class="menu">
                <ul id="saturday">
                    <li>@Html.ActionLink("Generate DML", "FileUpload", "Home")</li>
                    <li>@Html.ActionLink("Add Table", "AddTable", "Home")</li>
                    <li>@Html.ActionLink("Add Scheme", "AddScheme", "Home")</li>
                </ul>
            </div>
        </div>
    <div class="body">
        <div class = "content">
            @RenderBody()
        </div>
    </div>
</body>
</html>
