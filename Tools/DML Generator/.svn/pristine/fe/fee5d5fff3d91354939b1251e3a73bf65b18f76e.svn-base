/*Header Starts*/
.header
{
    background: #549635 url('images/header-background.png') no-repeat right; opacity:80;
    color: White;
    height:100px;
}
.header .title 
{ 
    font-size: 2em;
    padding: .6em;
    font-family: "Trebuchet MS", Verdana, sans-serif;
    text-align:center;
}
    

DIV.title span 
{
    font-size: .5em;
    font-family: Verdana;
    height:37px;
}
/*Header Ends*/
.input
{
    display:block;
    margin-top: 5px;    
}
.bluebox
{
    border:2px solid #CCEDFF;
    background-color:#E5F6FF;
    padding:20px;
    margin:20px;
    box-shadow: 0 0 6px rgba(0, 0, 0, .7);
}

.bluebox h4
{
    margin-bottom: 10px;
    margin-top: 10px;
}

.sheetInfo
{
    display:block;
    width:100%;
    border-bottom: grey dotted 1px;
}

.messageInfo
{
    display:block;
    width:100%;
}

.errorMessage
{
    font-family:Monospace;
    color: Red;
    display: none;
    padding-left: 16px;
    border-top: grey dashed 1px;
}

.sheetInfoHeader
{
    display:block;
    width:100%;
    border-bottom: grey solid 1px;
    font-weight:bold;
}

.sheetInfoHeader .cellContent
{
    font-family:Monospace;
    font-size:large;
}

.cellContent
{
    width: 17%;
    display: inline-block;
    padding-left:1%;
    font-family:Monospace;
}

.warning
{
    background: transparent url('images/warning.png') no-repeat -1px 0px;
    padding-left:16px;
    font-family:Monospace;
    color: Red;
}

.success
{
    background: transparent url('images/success.png') no-repeat -1px 0px;
    padding-left:16px;
    font-family:Monospace;
}

.contentDiv
{
    float:right;
}

.message
{
    float:left;
    margin-right: 10px;
    padding-top:10px;
    font-weight:bold;
    width:55%;
    font-style:oblique;
}

.inblock
{
    display:inline-block;    
}

.block
{
    display:block;    
    margin-bottom: 15px;
}

#download
{
    display:none;
}

/* Button Code*/
.button {
	display: inline-block;
	zoom: 1; /* zoom and *display = ie7 hack for display:inline-block */
	*display: inline;
	vertical-align: baseline;
	margin: 0 2px;
	outline: none;
	cursor: pointer;
	text-align: center;
	text-decoration: none;
	font: 14px/100% Arial, Helvetica, sans-serif;
	padding: .5em 2em .55em;
	text-shadow: 0 1px 1px rgba(0,0,0,.3);
	-webkit-border-radius: .5em; 
	-moz-border-radius: .5em;
	border-radius: .5em;
	-webkit-box-shadow: 0 1px 2px rgba(0,0,0,.2);
	-moz-box-shadow: 0 1px 2px rgba(0,0,0,.2);
	box-shadow: 0 1px 2px rgba(0,0,0,.2);
}
.button:hover {
	text-decoration: none;
}
.button:active {
	position: relative;
	top: 1px;
}

.bigrounded {
	-webkit-border-radius: 2em;
	-moz-border-radius: 2em;
	border-radius: 2em;
}
.medium {
	font-size: 12px;
	padding: .4em 1.5em .4em;
}
.small {
	font-size: 11px;
	padding: .2em 1em .275em;
}

/* black */
.black {
	color: #fff;
	border: solid 1px #333;
	background: #333;
	background: -webkit-gradient(linear, left top, left bottom, from(#666), to(#000));
	background: -moz-linear-gradient(top,  #666,  #000);
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#666666', endColorstr='#000000');
}
.black:hover {
	background: #000;
	background: -webkit-gradient(linear, left top, left bottom, from(#444), to(#000));
	background: -moz-linear-gradient(top,  #444,  #000);
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#444444', endColorstr='#000000');
}
.black:active {
	color: #666;
	background: -webkit-gradient(linear, left top, left bottom, from(#000), to(#444));
	background: -moz-linear-gradient(top,  #000,  #444);
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#000000', endColorstr='#666666');
}

