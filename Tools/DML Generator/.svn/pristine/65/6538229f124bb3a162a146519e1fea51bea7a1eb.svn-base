using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using DML.Generator.Domain.Abstract;

namespace DML.Generator.Domain
{
    public class Observer : IObserver
    {
        ExcelFactory excel;
        string state;

        public Observer(ExcelFactory subject)
        {
            this.excel = subject;
            subject.Notify += Update;
        }

        public void Update(string subjectState)
        {
            state = subjectState;
            Console.WriteLine("Observer : " + state);
        }
    }
}
